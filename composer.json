{
    "name": "hummer2k/conlayout",
    "description": "Module for handling laminas-mvc layouts with blocks and handles similar to magento",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "keywords": [
        "zf",
        "zf3",
        "laminas",
        "layout",
        "handles",
        "blocks"
    ],
    "authors": [
        {
            "name": "Cornelius Adams",
            "email": "cornelius.adams@conlabz.de",
            "homepage": "http://www.conlabz.de/"
        }
    ],
    "require": {
        "php": "^7.3",
        "laminas/laminas-config": "^3.0",
        "laminas/laminas-dependency-plugin": "*",
        "laminas/laminas-eventmanager": "^3.0",
        "laminas/laminas-http": "^2.5",
        "laminas/laminas-modulemanager": "^2.7",
        "laminas/laminas-mvc": "^3.0",
        "laminas/laminas-router": "^3.0",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-stdlib": "^3.0",
        "laminas/laminas-view": "^2.11.3"
    },
    "suggest": {
        "laminas/laminas-filter": "Filter configured params e.g. src, href attributes (basepath, cachebuster, ...)"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "satooshi/php-coveralls": "~0.6",
        "squizlabs/php_codesniffer": "^3.5",
        "laminas/laminas-developer-tools": "^1.1",
        "laminas/laminas-filter": "^2.7.1",
        "laminas/laminas-i18n": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "ConLayout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConLayoutTest\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "component": "ConLayout"
        }
    }
}
