{
    "name": "hummer2k/conlayout",
    "description": "Module for handling zend-mvc layouts with blocks and handles similar to magento",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "keywords": [
        "zf",
        "zf3",
        "laminas",
        "layout",
        "handles",
        "blocks"
    ],
    "authors": [
        {
          "name": "Cornelius Adams",
          "email": "cornelius.adams@conlabz.de",
          "homepage": "http://www.conlabz.de/"
        }
    ],
    "require": {
        "php": "^7.2",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-router": "^3.0",
        "zendframework/zend-stdlib": "^3.0",
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-config": "^2.6",
        "zendframework/zend-view": "^2.11"
    },
    "suggest": {
        "zendframework/zend-filter": "Filter configured params e.g. src, href attributes (basepath, cachebuster, ...)"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "satooshi/php-coveralls": "~0.6",
        "squizlabs/php_codesniffer": "^3.5",
        "zendframework/zend-developer-tools": "^1.1",
        "zendframework/zend-filter": "^2.7.1",
        "zendframework/zend-i18n": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "ConLayout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConLayoutTest\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "component": "ConLayout"
        }
    }
}
