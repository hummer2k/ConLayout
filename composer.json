{
    "name": "hummer2k/conlayout",
    "description": "Module for handling ZF2 layouts with blocks and handles like magento",
    "license": "MIT",
    "keywords": [
        "zf2",
        "layout",
        "handles",
        "blocks"
    ],
    "authors": [
        {
          "name": "Cornelius Adams",
          "email": "cornelius.adams@conlabz.de",
          "homepage": "http://www.conlabz.de/"
        }
    ],
    "repositories": [
        {
            "type" : "vcs",
            "url" : "git@github.com:samsonasik/ZendDeveloperTools.git"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-mvc": "3.*",
        "zendframework/zend-servicemanager": "3.*",
        "zendframework/zend-eventmanager": "3.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-router": "3.*",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-stdlib": "3.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-config": "2.*",
        "zendframework/zend-view": "2.*"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "dev-zf3",
        "phpunit/phpunit": "4.*",
        "zendframework/zend-console": "2.*",
        "zendframework/zend-i18n": "2.*",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4": {
            "ConLayout\\": "src/",
            "ConLayoutTest\\": "test/"
        }
    }
}
