{
    "name": "hummer2k/conlayout",
    "description": "Module for handling ZF2 layouts with blocks and handles like magento",
    "license": "MIT",
    "keywords": [
        "zf2",
        "layout",
        "handles",
        "blocks"
    ],
    "authors": [
        {
          "name": "Cornelius Adams",
          "email": "cornelius.adams@conlabz.de",
          "homepage": "http://www.conlabz.de/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-serializer": "2.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-i18n": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-config": "2.*",
        "zendframework/zend-console": "2.*"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "dev-master",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-0": {
            "ConLayout\\": "src/"
        }
    }
}
