{
    "name": "hummer2k/conlayout",
    "description": "Module for handling ZF2 layouts with blocks and handles like magento",
    "license": "MIT",
    "keywords": [
        "zf2",
        "layout",
        "handles",
        "blocks"
    ],
    "authors": [
        {
          "name": "Cornelius Adams",
          "email": "cornelius.adams@conlabz.de",
          "homepage": "http://www.conlabz.de/"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-modulemanager": "^2.7 || ^3.0",
        "zendframework/zend-router": "^3.0",
        "zendframework/zend-filter": "^2.7.1 || ^3.0",
        "zendframework/zend-stdlib": "^3.0",
        "zendframework/zend-http": "^2.0 || ^3.0",
        "zendframework/zend-config": "^2.6 || ^3.0",
        "zendframework/zend-view": "^2.6 || ^3.0"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "^1.1",
        "phpunit/phpunit": "4.*",
        "zendframework/zend-console": "^2.6 || ^3.0",
        "zendframework/zend-i18n": "^2.7 || ^3.0",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4": {
            "ConLayout\\": "src/",
            "ConLayoutTest\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "module": "ConLayout"
        }
    }
}
